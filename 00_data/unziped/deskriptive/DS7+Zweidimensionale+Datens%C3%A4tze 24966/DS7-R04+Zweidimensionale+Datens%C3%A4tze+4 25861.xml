            <hasVisibleFeedback>false</hasVisibleFeedback>
            <isAsync>false</isAsync>
            <weight>1</weight>
            <persistentReference>25866</persistentReference>
          </checkerConfiguration>
        </TestCaseTuple>
      </testCasetuples>
