              <ruleMode>PRESENCE</ruleMode>
              <queries class="linked-list" id="28">
                <RGReQLQuery id="29">
                  <query>		from cqt:V{FunctionCall}, kt:V{Variable} with 
                cqt.name=&quot;chisq.test&quot; and
                kt.name=&quot;kt&quot;
                report 0 end
		</query>
                </RGReQLQuery>
