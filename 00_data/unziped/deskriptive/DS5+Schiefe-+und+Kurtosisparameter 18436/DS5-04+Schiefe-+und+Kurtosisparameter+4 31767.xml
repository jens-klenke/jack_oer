          <text> Die Verteilung ist linkssteil</text>
          <variableName>p1</variableName>
          <mcstage reference="6"/>
        </MCAnswer>
        <MCAnswer id="29">
          <order>1</order>
          <rule>VARIABLE</rule>
          <text> Die Verteilung ist rechtssteil</text>
          <variableName>p2</variableName>
          <mcstage reference="6"/>
        </MCAnswer>
        <MCAnswer id="30">
          <order>2</order>
          <rule>VARIABLE</rule>
          <text> Die Verteilung ist linksschief</text>
          <variableName>p3</variableName>
          <mcstage reference="6"/>
        </MCAnswer>
