  <variableDeclarations id="39">
    <VariableDeclaration id="40">
      <name>i</name>
      <initializationCode id="41">
        <code>evaluateInR(&apos;sample(0:10, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="42">
      <name>Ee</name>
      <initializationCode id="43">
        <code>getFromList([var=i], list(20,20,20,20,20,40,40,40,10,30,12))</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="44">
      <name>Var</name>
      <initializationCode id="45">
        <code>getFromList([var=i], list(19,18,16,15,10,38,36,30,9,27,9))</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="46">
      <name>multi</name>
      <initializationCode id="47">
        <code>evaluateInR(&apos;sample(2:7, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="48">
      <name>Add</name>
      <initializationCode id="49">
        <code>evaluateInR(&apos;sample(15:45, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="50">
      <name>EY</name>
      <initializationCode id="51">
        <code>([var=multi])*([var=Ee])-([var=Add])</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="52">
      <name>VarY</name>
      <initializationCode id="53">
        <code>([var=Var])+([var=Ee])*([var=Ee])</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="54">
      <name>piu</name>
      <initializationCode id="55">
        <code>1-(([var=Var])/([var=Ee]))</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="56">
      <name>pi</name>
      <initializationCode id="57">
        <code>round(([var=piu]), 2)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="58">
      <name>nu</name>
      <initializationCode id="59">
        <code>([var=Ee])/([var=pi])</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="60">
      <name>n</name>
      <initializationCode id="61">
        <code>round([var=nu])</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
  </variableDeclarations>
