    </VariableDeclaration>
    <VariableDeclaration id="39">
      <name>ab</name>
      <initializationCode id="40">
        <code>evaluateInR(&apos;round(runif(1, 2, 2.25), 2)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="41">
      <name>abs</name>
      <initializationCode id="42">
        <code>evaluateInR(&apos;round(runif(1, 0.7, 1.2), 2)&apos;)</code>
