  <variableDeclarations id="66">
    <VariableDeclaration id="67">
      <name>gezinktper</name>
      <initializationCode id="68">
        <code>randomIntegerBetween(50,80)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="69">
      <name>verlierenper</name>
      <initializationCode id="70">
        <code>randomIntegerBetween(60,95)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="71">
      <name>gezinkt</name>
      <initializationCode id="72">
        <code>([var=gezinktper])/100</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="73">
      <name>verlieren</name>
      <initializationCode id="74">
        <code>([var=verlierenper])/100</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="75">
      <name>sol1</name>
      <initializationCode id="76">
        <code>([var=gezinkt]*[var=verlieren])/([var=gezinkt]*[var=verlieren]+(1-[var=gezinkt])*0.5)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="77">
      <name>rsol1</name>
      <initializationCode id="78">
        <code>round(([var=sol1]), 4)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="79">
      <name>sol2</name>
      <initializationCode id="80">
        <code>([var=rsol1]*[var=verlieren])/([var=rsol1]*[var=verlieren]+(1-[var=rsol1])*0.5)</code>
