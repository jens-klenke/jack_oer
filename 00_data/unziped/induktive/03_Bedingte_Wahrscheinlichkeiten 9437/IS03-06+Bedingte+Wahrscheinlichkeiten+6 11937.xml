  <variableDeclarations id="68">
    <VariableDeclaration id="69">
      <name>infected</name>
      <initializationCode id="70">
        <code>randomIntegerBetween(1,5)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="71">
      <name>infec</name>
      <initializationCode id="72">
        <code>evaluateInR(&apos;[var=infected]/100&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="73">
      <name>i1</name>
      <initializationCode id="74">
        <code>randomIntegerBetween(0,3)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="75">
      <name>sens</name>
      <initializationCode id="76">
        <code>getFromList([var=i1], list(0.9,0.95,0.99,0.999))</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="77">
      <name>sensitivity</name>
      <initializationCode id="78">
        <code>evaluateInR(&apos;[var=sens]*100&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="79">
      <name>j</name>
      <initializationCode id="80">
        <code>randomIntegerBetween(0,3)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="81">
      <name>spec</name>
      <initializationCode id="82">
        <code>getFromList([var=j], list(0.01,0.05,0.1,0.2))</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="83">
      <name>specificity</name>
      <initializationCode id="84">
        <code>evaluateInR(&apos;[var=spec]*100&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="85">
      <name>sol1</name>
      <initializationCode id="86">
        <code>evaluateInR(&apos;(([var=sens]*[var=infec])/([var=sens]*[var=infec]+[var=spec]*(1-[var=infec])))&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="87">
      <name>sol2</name>
      <initializationCode id="88">
        <code>evaluateInR(&apos;(((1-[var=sens])*[var=infec])/((1-[var=sens])*[var=infec]+(1-[var=spec])*(1-[var=infec])))&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="89">
      <name>sol11</name>
      <initializationCode id="90">
        <code>round([var=sol1], 4)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="91">
