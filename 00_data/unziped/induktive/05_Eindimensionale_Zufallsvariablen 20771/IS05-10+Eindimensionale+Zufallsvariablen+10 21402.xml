  <variableDeclarations id="36">
    <VariableDeclaration id="37">
      <name>Ee</name>
      <initializationCode id="38">
        <code>randomIntegerBetween(15,25)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="39">
      <name>V1</name>
      <initializationCode id="40">
        <code>randomIntegerBetween(100,300)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="41">
      <name>V2</name>
      <initializationCode id="42">
        <code>([var=V1])/10000</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="43">
      <name>V</name>
      <initializationCode id="44">
        <code>round(([var=V2]), 4)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="45">
      <name>A1</name>
      <initializationCode id="46">
        <code>randomIntegerBetween(2,8)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="47">
      <name>A2</name>
      <initializationCode id="48">
        <code>([var=A1])/10</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="49">
      <name>A</name>
      <initializationCode id="50">
        <code>round(([var=A2]), 1)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="51">
      <name>Ober1</name>
      <initializationCode id="52">
        <code>([var=Ee])+([var=A])</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="53">
      <name>Ober</name>
      <initializationCode id="54">
        <code>round(([var=Ober1]), 1)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="55">
      <name>Unter1</name>
      <initializationCode id="56">
        <code>([var=Ee])-([var=A])</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="57">
      <name>Unter</name>
      <initializationCode id="58">
        <code>round(([var=Unter1]), 1)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="59">
      <name>c1</name>
      <initializationCode id="60">
        <code>[var=Ober]-[var=Ee]</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="61">
      <name>c</name>
      <initializationCode id="62">
        <code>round(([var=c1]), 1)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="63">
      <name>ans1</name>
      <initializationCode id="64">
        <code>([var=V])/pow([var=A], 2)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="65">
      <name>ans</name>
      <initializationCode id="66">
        <code>round(([var=ans1]), 4)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
  </variableDeclarations>
