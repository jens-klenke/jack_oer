      <testCasetuples id="26">
        <TestCaseTuple id="27">
          <testCases id="28">
            <StaticRTestCase id="29">
              <points>100</points>
              <pointsMode>GAIN</pointsMode>
              <name>imported testcase</name>
              <feedbackIfFailed>Leider nicht richtig! Achten Sie darauf, dass Sie die Variable $\verb|wert|$_$\verb|aktuell|$ nennen und die vordefinierten Variablennamen richtig geschrieben haben.
</feedbackIfFailed>
              <ruleMode>PRESENCE</ruleMode>
              <queries class="linked-list" id="30">
                <RGReQLQuery id="31">
                  <query>		from lao:V{LeftAssignmentOperator}, wert,inv,ren,jah:V{Variable}, mul:V{MulOperator}, exp:V{ExpOperator}, e1:E{LeftHandSide}, e2:E{RightHandSide}, e3,e4:E{Expression} with 
                lao -- e1 -&gt; wert and
                wert.name=&quot;wert_aktuell&quot; and
                lao -- e2 -&gt; mul and
                mul --&gt; inv and
                inv.name=&quot;investment&quot; and
                mul --&gt; exp and
                exp -- e3 -&gt; ren and
                exp -- e4 -&gt; jah and
                e3.position &lt; e4.position and
                ren.name=&quot;rendite&quot; and
                jah.name=&quot;jahre&quot;
		report 0 end
		</query>
                </RGReQLQuery>
              </queries>
            </StaticRTestCase>
          </testCases>
