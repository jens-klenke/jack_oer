			&amp;nbsp;&lt;input name=&quot;field10&quot; size=&quot;6&quot; type=&quot;text&quot; value=&quot;field10&quot; /&gt;&amp;nbsp;&lt;/td&gt;&#xd;
			&lt;td style=&quot;text-align: center;&quot;&gt;&lt;br /&gt;&#xd;
