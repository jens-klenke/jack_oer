			&lt;td style=&quot;text-align: right;&quot;&gt;&lt;input name=&quot;field9&quot; size=&quot;6&quot; type=&quot;text&quot; value=&quot;field9&quot; /&gt;&lt;/td&gt;&#xd;
		&lt;/tr&gt;&#xd;
		&lt;tr&gt;&#xd;
			&lt;td&gt;&lt;br /&gt;&#xd;
