			&lt;td style=&quot;text-align: center;&quot;&gt;&lt;br /&gt;&#xd;
			&amp;nbsp;&lt;input name=&quot;field11&quot; size=&quot;6&quot; type=&quot;text&quot; value=&quot;field11&quot; /&gt;&amp;nbsp;&lt;/td&gt;&#xd;
			&lt;td style=&quot;text-align: right;&quot;&gt;&lt;br /&gt;&#xd;
			100&lt;/td&gt;&#xd;
		&lt;/tr&gt;&#xd;
