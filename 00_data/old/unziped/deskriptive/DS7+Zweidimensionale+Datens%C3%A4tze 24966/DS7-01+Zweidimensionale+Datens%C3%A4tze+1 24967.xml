			&lt;td style=&quot;text-align: right;&quot;&gt;&amp;nbsp;&lt;input name=&quot;field4&quot; size=&quot;7&quot; type=&quot;text&quot; value=&quot;field4&quot; /&gt;&amp;nbsp;&lt;/td&gt;&#xd;
		&lt;/tr&gt;&#xd;
	&lt;/tbody&gt;&#xd;
&lt;/table&gt;&#xd;
&lt;br /&gt;&#xd;
&lt;br /&gt;&#xd;
