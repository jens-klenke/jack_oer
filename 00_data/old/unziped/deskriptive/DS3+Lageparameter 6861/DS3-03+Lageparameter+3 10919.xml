  <variableDeclarations id="50">
    <VariableDeclaration id="51">
      <name>a</name>
      <initializationCode id="52">
        <code>randomIntegerBetween(20,30)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="53">
      <name>b</name>
      <initializationCode id="54">
        <code>randomIntegerBetween(3,5)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="55">
      <name>c</name>
      <initializationCode id="56">
        <code>randomIntegerBetween(2,5)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="57">
      <name>modus</name>
      <initializationCode id="58">
        <code>[var=a]*100</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="59">
      <name>median</name>
      <initializationCode id="60">
        <code>([var=a]+[var=b])*100</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="61">
      <name>mittel</name>
      <initializationCode id="62">
        <code>([var=a]+[var=b]+[var=c])*100</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="63">
      <name>d</name>
      <initializationCode id="64">
        <code>randomIntegerBetween(20,100)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="65">
      <name>z1</name>
      <initializationCode id="66">
        <code>[var=median]+([var=d]*10)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="67">
      <name>e</name>
      <initializationCode id="68">
        <code>randomIntegerBetween(100,130)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="69">
      <name>z2</name>
      <initializationCode id="70">
        <code>[var=median]+([var=e]*10)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="71">
      <name>f</name>
      <initializationCode id="72">
        <code>randomIntegerBetween(20,100)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="73">
      <name>z3</name>
      <initializationCode id="74">
        <code>[var=median]-([var=f]*10)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="75">
      <name>g</name>
      <initializationCode id="76">
        <code>randomIntegerBetween(100,130)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="77">
      <name>z4</name>
      <initializationCode id="78">
        <code>[var=median]-([var=g]*10)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="79">
      <name>mittel_neu</name>
      <initializationCode id="80">
        <code>round((([var=mittel]*130)+[var=z1]+[var=z2]+[var=z3]+[var=z4])/134)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
  </variableDeclarations>
