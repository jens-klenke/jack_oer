  <variableDeclarations id="98">
    <VariableDeclaration id="99">
      <name>st</name>
      <initializationCode id="100">
        <code>evaluateInR(&apos;sample(5:7/100, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="101">
      <name>Ev</name>
      <initializationCode id="102">
        <code>evaluateInR(&apos;sample(47:49/100, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="103">
      <name>n</name>
      <initializationCode id="104">
        <code>randomIntegerBetween(25,30)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="105">
      <name>sig</name>
      <initializationCode id="106">
        <code>evaluateInR(&apos;sample(c(0.01, 0.05), 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="107">
      <name>sigP</name>
      <initializationCode id="108">
        <code>[var=sig]*100</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="109">
      <name>crit</name>
      <initializationCode id="110">
        <code>evaluateInR(&apos;round(qnorm([var=sig]), 4)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="111">
      <name>P</name>
      <initializationCode id="112">
        <code>evaluateInR(&apos;(([var=Ev])-0.5)/(([var=st])/sqrt(([var=n])))&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="113">
      <name>Pg</name>
      <initializationCode id="114">
        <code>evaluateInR(&apos;round(([var=P]), 4)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="115">
      <name>Ent</name>
      <initializationCode id="116">
        <code>evaluateInR(&apos;ifelse(([var=P])&lt;qnorm(([var=sig])), 1, 0)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="117">
      <name>Entstr</name>
      <initializationCode id="118">
        <code>evaluateInR(&apos;ifelse([var=Ent] == 1, &quot;verworfen&quot;, &quot;nicht verworfen&quot;)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="119">
      <name>xkrit</name>
      <initializationCode id="120">
        <code>evaluateInR(&apos;round(0.5+qnorm(([var=sig]))*(([var=st])/sqrt(([var=n]))), 4)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="121">
      <name>niv</name>
      <initializationCode id="122">
        <code>evaluateInR(&apos;round(pnorm(([var=P])), 4)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
  </variableDeclarations>
