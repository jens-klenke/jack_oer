      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="72">
      <name>ansa</name>
      <initializationCode id="73">
        <code>evaluateInR(&apos;round(2*(1-pnorm(8/sqrt([var=varianz]/[var=n]), 0, 1)), 4)&apos;)</code>
