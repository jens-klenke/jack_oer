  <variableDeclarations id="125">
    <VariableDeclaration id="126">
      <name>prozent</name>
      <initializationCode id="127">
        <code>evaluateInR(&apos;sample(5:10, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="128">
      <name>n</name>
      <initializationCode id="129">
        <code>evaluateInR(&apos;sample(10:30, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="130">
      <name>Ertrag</name>
      <initializationCode id="131">
        <code>evaluateInR(&apos;sample(8:12, 1)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="132">
      <name>std</name>
      <initializationCode id="133">
        <code>evaluateInR(&apos;round(runif(1, 0.1, 0.3), 2)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="134">
      <name>mean</name>
      <initializationCode id="135">
        <code>evaluateInR(&apos;round(rnorm(1, 9, 3), 2)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="136">
      <name>mu01</name>
      <initializationCode id="137">
        <code>evaluateInR(&apos;1-([var=prozent]/100)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="138">
      <name>mu0</name>
      <initializationCode id="139">
        <code>[var=Ertrag]*[var=mu01]</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="140">
      <name>Z</name>
      <initializationCode id="141">
        <code>evaluateInR(&apos;round(([var=mean]-[var=mu0])/([var=std]/sqrt([var=n])), 4)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="142">
      <name>quantile</name>
      <initializationCode id="143">
        <code>evaluateInR(&apos;round(qnorm(0.90), 4)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
    <VariableDeclaration id="144">
      <name>decision</name>
      <initializationCode id="145">
        <code>evaluateInR(&apos;ifelse([var=Z]&gt;[var=quantile], 1, 0)&apos;)</code>
        <domain>MATH</domain>
      </initializationCode>
    </VariableDeclaration>
  </variableDeclarations>
